              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "Success"
                    },
                    "data": {
                      "type": "object",
                      "properties": {
                        "newUser": {
                          "type": "object",
                          "properties": {
                            "name": {
                              "type": "string",
                              "example": "fathan"
                            },
                            "email": {
                              "type": "email",
                              "example": "fathan5@mail.com"
                            },
                            "password": {
                              "type": "password",
                              "example": "$2b$10$pg0ivHyurpBkVjUkO5LDUegnyP42nC3bPdNOk0ECaT0zsdgDiHASa"
                            },
                            "warehouseId": {
                              "type": "integer",
                              "example": "12"
                            },
                            "updatedAt": {
                              "type": "date",
                              "example": "2022-10-17T14:04:48.389Z"
                            },
                            "createdAt": {
                              "type": "date",
                              "example": "2022-10-17T14:04:48.389Z"
                            }
                          }
                        },
                        "warehouse": {
                          "type": "object",
                          "properties": {
                            "id": {
                              "type": "integer",
                              "example": "12"
                            },
                            "warehouseName": {
                              "type": "string",
                              "example": "Toko Fathan 5"
                            },
                            "updatedAt": {
                              "type": "date",
                              "example": "2022-10-17T14:04:48.389Z"
                            },
                            "createdAt": {
                              "type": "date",
                              "example": "2022-10-17T14:04:48.389Z"
                            },
                            "address": {
                              "type": "string",
                              "example": null
                            }
                          }
                        },
                        "newWarehouse": {
                          "type": "boolean",
                          "example": true
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Minimum password not enough",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "failed"
                    },
                    "message": {
                      "type": "string",
                      "example": "minimum password length must be 8 charater or more"
                    }
                  }
                }
              }
            }
          },
          "400*": {
            "description": "user already exist",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "failed"
                    },
                    "message": {
                      "type": "string",
                      "example": "email already exist!"
                    }
                  }
                }
              }
            }
          },
          "409": {
            "description": "Warehouse Name already exist",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "failed"
                    },
                    "message": {
                      "type": "string",
                      "example": "warehouse Name already taken yeah !"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
