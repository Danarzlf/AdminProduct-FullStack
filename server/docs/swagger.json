    "/api/v1/auth/login": {
      "post": {
        "tags": ["Users"],
        "requestBody": {
          "description": "This is Login API",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "email": {
                    "type": "email",
                    "example": "fathan5@mail.com"
                  },
                  "password": {
                    "type": "password",
                    "example": "fathancintadia"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success Login",
            "schema": "#/components/schemas/User",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "Success"
                    },
                    "data": {
                      "type": "object",
                      "properties": {
                        "username": {
                          "type": "string",
                          "example": "fathan"
                        },
                        "email": {
                          "type": "email",
                          "example": "fathan5@mail.com"
                        },
                        "warehouse": {
                          "type": "String",
                          "example": "Toko Fathan 5"
                        },
                        "token": {
                          "type": "string",
                          "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6MTksIndhcmVob3VzZUlkIjoxMiwiaWF0IjoxNjY2MDIyNTc0fQ.HpBlFpKLYexC8uw5Qps6yfNcX_m8OinmqUO4NLPQgHs"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Wrong Password",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "example": "failed"
                    },
                    "message": {
                      "type": "string",
                      "example": "Wrong Password"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/v1/auth/register": {
      "post": {
        "tags": ["Users"],
        "requestBody": {
          "description": "Success registered new user",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "fathan"
